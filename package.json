{
    "name": "privacywallet",
    "version": "0.1.0",
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm docs/*.*",
        "dev": "snowpack dev",
        "edev": "esbuild src/app.js src/welcome.js --loader:.png=dataurl --bundle --servedir=docs --outdir=docs",
        "build:light": "./ebuild.sh",
        "workbox": "workbox generateSW workbox-config.js",
